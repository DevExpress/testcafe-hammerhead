{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "strictFunctionTypes": true,
    "strict": false,
    "newLine": "lf"
  },
  "exclude": [
    "node_modules"
  ]
}
